{
  "name": "InterviewCopilot",
  "version": "1.0.11",
  "description": "AI-powered interview preparation assistant",
  "main": "./out/main/index.js",
  "author": "Interview Copilot <support@interviewcopilot.app>",
  "homepage": "https://interviewcopilot.app",
  "scripts": {
    "dev": "electron-vite dev",
    "start": "electron-vite preview",
    "build": "electron-vite build && yarn build:asar && yarn build:app",
    "build:vite": "electron-vite build",
    "build:asar": "npx @electron/asar pack . ../builds/InterviewCopilot.asar --unpack '*.{node,dylib}'",
    "build:app": "cd ../builds && rm -rf InterviewCopilot.app && cp -R /Applications/InterviewCopilot.app InterviewCopilot.app && cp InterviewCopilot.asar InterviewCopilot.app/Contents/Resources/app.asar && cp -R InterviewCopilot.asar.unpacked InterviewCopilot.app/Contents/Resources/ && sudo xattr -cr InterviewCopilot.app && sudo codesign --force --deep --sign \"Developer ID Application: Md Miah (UJ2X7H6A7E)\" InterviewCopilot.app",
    "open": "open ../builds/InterviewCopilot.app",
    "kill": "pkill -f 'electron' || true",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dist": "electron-vite build && electron-builder --mac",
    "dist:dmg": "electron-vite build && electron-builder --mac dmg",
    "dist:zip": "electron-vite build && electron-builder --mac zip"
  },
  "build": {
    "appId": "com.interviewcopilot.app",
    "productName": "Interview Copilot",
    "directories": {
      "output": "../builds/dist"
    },
    "files": [
      "out/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icons/icon-1024.png",
      "identity": "Md Miah (UJ2X7H6A7E)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "extraResources": [
      {
        "from": "node_modules/@img/sharp-libvips-darwin-arm64/lib",
        "to": "sharp-libvips",
        "filter": [
          "*.dylib"
        ]
      },
      {
        "from": "node_modules/better-sqlite3/build/Release",
        "to": "better-sqlite3",
        "filter": [
          "*.node"
        ]
      }
    ],
    "asarUnpack": [
      "node_modules/sharp/**/*",
      "node_modules/@img/sharp-libvips-darwin-arm64/**/*"
    ]
  },
  "dependencies": {
    "@deepgram/sdk": "^3.13.0",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/electron": "^5.12.0",
    "@sentry/vite-plugin": "^3.6.1",
    "@stripe/stripe-js": "^1.54.2",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/vite": "^4.1.14",
    "@themosaad/tailwindcss-capsize": "^1.0.0",
    "@types/wav": "^1.0.4",
    "@vercel/blob": "^0.22.3",
    "ai": "^2.2.37",
    "assemblyai": "^4.16.1",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "electron-notarize": "^1.2.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "express": "^4.21.2",
    "hast": "^1.0.0",
    "latex.js": "^0.12.6",
    "lucide-react": "^0.475.0",
    "mermaid": "^10.9.4",
    "openai-edge": "^1.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-keyed-flatten-children": "^2.2.1",
    "react-markdown": "^9.1.0",
    "react-qr-code": "^2.0.18",
    "recordrtc": "^5.6.2",
    "rehype-highlight": "^6.0.0",
    "rehype-react": "^8.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^11.1.2",
    "screenshot-desktop": "^1.15.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@electron/asar": "^3.4.1",
    "@electron/rebuild": "^4.0.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsdom": "^27",
    "@types/node": "^20.19.21",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.8",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.3.0",
    "jsdom": "^27.1.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.20",
    "vitest": "^4.0.8"
  },
  "packageManager": "yarn@4.1.0"
}
